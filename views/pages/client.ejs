  <main>
    <article>

      <!-- 
        - HERO
      -->

      <section class="hero" id="hero">
        <div class="container">

          <div class="hero-content">
            <h1 class="h1 hero-title">Cameroun</h1>

            <p class="hero-text">
                The platform for the creation of your civil act and identification document
